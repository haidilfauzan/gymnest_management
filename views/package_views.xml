<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="gym_packages_list" model="ir.ui.view">
        <field name="name">gym.packages.list</field>
        <field name="model">gym.packages</field>
        <field name="arch" type="xml">
            <list string="Gyms">
                <field name="name"/>
                 <field name="package_type"/>
                <field name="price"/>
                <field name="duration"/>
                <field name="unit_duration"/>
                <field name="is_active"/>
            </list>
        </field>
    </record>


    <record id="action_packages_type" model="ir.actions.act_window">
        <field name="name">Packages Type</field>
        <field name="res_model">packages.type</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="action_packages" model="ir.actions.act_window">
        <field name="name">Packages</field>
        <field name="res_model">gym.packages</field>
        <field name="view_mode">list,form</field>
    </record>

    </odoo>